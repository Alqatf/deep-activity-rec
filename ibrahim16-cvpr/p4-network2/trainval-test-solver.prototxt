net: "examples/deep-activity-rec/ibrahim16-cvpr/p4-network2/trainval-test-network.prototxt"

# testing examples are 13370 = 1337 * 10.
test_iter: 1337
test_interval: 2000
display: 2000

base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 10000
max_iter: 20000
momentum: 0.9
weight_decay: 0.0005

random_seed: 750301
solver_mode: GPU

snapshot: 5000
snapshot_prefix: "examples/deep-activity-rec/ibrahim16-cvpr/p4-network2/z_snapshot"
snapshot_after_train: true

