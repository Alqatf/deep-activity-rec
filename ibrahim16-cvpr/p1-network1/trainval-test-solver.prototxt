net: "examples/deep-activity-rec/ibrahim16-cvpr/p1-network1/trainval-test-network.prototxt"

# testing examples are 77655 ~= 250 * 310
test_iter: 310
test_interval: 15000
display: 1000

base_lr: 0.00001
lr_policy: "step"
gamma: 0.1
stepsize: 15000
max_iter: 15000
momentum: 0.9
weight_decay: 0.0005

random_seed: 750301
solver_mode: GPU

snapshot: 5000
snapshot_prefix: "examples/deep-activity-rec/ibrahim16-cvpr/p1-network1/z_snapshot"
snapshot_after_train: true

