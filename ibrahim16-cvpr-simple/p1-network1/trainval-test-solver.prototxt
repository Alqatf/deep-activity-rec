net: "examples/deep-activity-rec/ibrahim16-cvpr-simple/p1-network1/trainval-test-network.prototxt"

# testing examples are 77655 ~= 250 * 310
test_iter: 1
test_interval: 1
display: 1

base_lr: 0.00001
lr_policy: "step"
gamma: 0.1
stepsize: 1
max_iter: 1
momentum: 0.9
weight_decay: 0.0005

random_seed: 750301
solver_mode: GPU

snapshot: 1
snapshot_prefix: "examples/deep-activity-rec/ibrahim16-cvpr-simple/p1-network1/z_snapshot"
snapshot_after_train: true

